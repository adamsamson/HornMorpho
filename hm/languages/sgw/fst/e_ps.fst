# delete or convert to e the vowel in passive prefix t*

-> start

start -> start  [X;V-*;%]

# * -> e when there's no prefix
start -> e      [e:*]
e -> te         [t]
te -> end       [=]

# * -> 0 when there's a prefix (or causative a)
start -> 0      [:*]
0 -> t          [t]
t -> pre        [=]
# a or maybe e if the = has been deleted
t -> start      [V]
pre -> start    [X;V]

end ->
start ->